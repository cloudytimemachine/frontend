    "react-bootstrap": "^0.30.3",
    "react-router": "^2.8.1",
    "react-router-bootstrap": "^0.23.1",
    "shelljs": "^0.7.3",
    "superagent": "^2.3.0"
  },
  "devDependencies": {
    "babel-core": "^6.13.2",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "babel-register": "^6.14.0",
    "babel-runtime": "^6.11.6",
    "chai": "^3.5.0",
    "enzyme": "^2.4.1",
    "jsdom": "^9.5.0",
