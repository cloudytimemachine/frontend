{
  "name": "static_server",
  "version": "1.0.0",
  "description": "Static http server and client-side javascript for cloudy time machine.",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "build-prod": "webpack",
    "dev": "webpack-dev-server --content-base public --history-api-fallback --define apiForwardingUrl='\"http://localhost:3001\"'",
